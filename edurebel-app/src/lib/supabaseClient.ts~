# (These exports are ONLY for this shell so you can test curl easily)
export NEXT_PUBLIC_SUPABASE_URL=https://mtqqrccaukvhplftsbd1.supabase.co
export NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_FULL_ANON_KEY

